<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape>
      <gradient
           android:startColor="#f61414"
           android:endColor="#e61414"
           android:angle="270" />
     <stroke
          android:width="1dp"
          android:color="#d3d3d3" />
